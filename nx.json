{
    "npmScope": "nx-additions",
    "affected": {
        "defaultBase": "main"
    },
    "implicitDependencies": {
        "package.json": {
            "dependencies": "*",
            "devDependencies": "*"
        },
        ".eslintrc.json": "*"
    },
    "tasksRunnerOptions": {
        "default": {
            "runner": "nx/tasks-runners/default",
            "options": {
                "cacheableOperations": ["build", "lint", "test", "e2e"],
                "parallel": 1
            }
        }
    },
    "workspaceLayout": {
        "appsDir": "packages",
        "libsDir": "packages"
    },
    "targetDependencies": {
        "build": [
            {
                "target": "build",
                "projects": "dependencies"
            }
        ]
    },
    "cli": {
        "defaultCollection": "@nrwl/workspace"
    }
}
